{
    "shells-bash-only": {
        "image": "ubuntu:latest",
        "features": {
            "ohmyposh": {
                "shells": "bash"
            }
        }
    },
    "theme": {
        "image": "ubuntu:latest",
        "features": {
            "ohmyposh": {
                "theme": "dracula"
            }
        }
    },
    "version": {
        "image": "ubuntu:latest",
        "features": {
            "ohmyposh": {
                "version": "v23.0.0"
            }
        }
    },
    "custom-install-path": {
        "image": "ubuntu:latest",
        "features": {
            "ohmyposh": {
                "installPath": "/usr/bin"
            }
        }
    }
}
